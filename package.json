{
	"name": "svelte-reorderable-list",
	"version": "0.2.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"svelte-package": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"src/lib"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./reset.css": "./dist/styles/reset.css"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.22.4",
		"@sveltejs/package": "^2.3.12",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"publint": "^0.3.12",
		"sass": "^1.89.2",
		"sass-embedded": "^1.89.2",
		"svelte": "^5.35.5",
		"svelte-check": "^4.2.2",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.8.3",
		"vite": "^6.0.0"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"svelte5",
		"reorderable",
		"list",
		"sortable",
		"tree",
		"nested",
		"hierarchical",
		"drag",
		"drop",
		"drag-and-drop",
		"dnd",
		"component",
		"ui",
		"components",
		"library",
		"typescript",
		"accessible",
		"keyboard",
		"a11y",
		"touch",
		"mobile",
		"ordering",
		"rearrange",
		"move"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LexRiver/svelte-reorderable-list.git"
	},
	"homepage": "https://github.com/LexRiver/svelte-reorderable-list",
	"bugs": {
		"url": "https://github.com/LexRiver/svelte-reorderable-list/issues"
	}
}